  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "form-data": "^2.3.2",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "node-fetch-polyfill": "^2.0.6",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "urijs": "^1.19.1"
  },
  "peerDependencies": {
    "urijs": "^1.19.1"
  }
}
